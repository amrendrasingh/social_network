<div class='container' style="margin-top:100px;">
	<%= form_for @user, url:  '/images',action: "post", :html => { :multipart => true } do |f| %>
	<h4>New Files</h4>
		<%= f.fields_for :images, :html => { :multipart => true } do |image| %>
  		<p>
				<%= image.file_field :photo %>
			</p>
		<% end %>
  	<p><%= f.submit 'Create Post',:class => "sub_btn" %></p>
	<% end %>
</div>